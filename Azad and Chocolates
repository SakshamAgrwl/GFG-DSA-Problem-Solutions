#include <iostream>
using namespace std;

int main() {
	//code
	int n,a;cin>>n;
	while(n--){
	    cin>>a;int flag=0;
	    for(int i=0;i<100;i+=7){
	        for(int j=0;j<100;j+=3){
	            if((100 - i-j) ==a){flag=1;cout<<1<<endl;break;}
	        }
	        if(flag){break;}
	    }
	    if(!flag){cout<<0<<endl;}
	}
	return 0;
}
